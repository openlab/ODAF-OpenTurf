    <sectionGroup name="bundleTransformer">
      <section name="core" type="BundleTransformer.Core.Configuration.CoreSettings, BundleTransformer.Core" />
      <section name="less" type="BundleTransformer.Less.Configuration.LessSettings, BundleTransformer.Less" />
    </sectionGroup>
    <sectionGroup name="jsEngineSwitcher">
      <section name="core" type="JavaScriptEngineSwitcher.Core.Configuration.CoreConfiguration, JavaScriptEngineSwitcher.Core" />
      <section name="msie" type="JavaScriptEngineSwitcher.Msie.Configuration.MsieConfiguration, JavaScriptEngineSwitcher.Msie" />
    </sectionGroup>
  </configSections>
  <system.diagnostics>
    <trace>
      <listeners>
        <add type="Microsoft.WindowsAzure.Diagnostics.DiagnosticMonitorTraceListener, Microsoft.WindowsAzure.Diagnostics, Version=2.4.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" name="AzureDiagnostics">
          <filter type="" />
        </add>
      </listeners>
    </trace>
  </system.diagnostics>
  <connectionStrings>
    <!-- CHANGEME: add your db connection string here -->
    <add name="ODAF" connectionString="[dbConnectionString]" providerName="System.Data.SqlClient" />
  </connectionStrings>
  
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!-- CHANGEME: add your Twitter Consumer Key here -->
    <add key="TwitterConsumerKey" value="[TwitterConsumerKey]" />
    <!-- CHANGEME: add your Twitter Consumer Key Secret here -->
    <add key="TwitterConsumerSecret" value="[TwitterConsumerSecret]" />
    <!-- CHANGEME: add your Federation Metadata Address here -->
    <add key="FederationMetadataAddress" value="https://login.windows.net/[AADName]/federationmetadata/2007-06/federationmetadata.xml" />
    <!-- CHANGEME: add your Federation Wtrealm here -->
    <add key="FederationWtrealm" value="http://[ServiceDNS].cloudapp.net/" />
    <!--<add key="FederationWtrealm" value="http://localhost:12094/"/>-->
    <!-- CHANGEME: add your Bing Map Key here -->
    <add key="BingCredential" value="[BingCredenntial]" />
  </appSettings>
  
  <system.web>
    <customErrors mode="Off"/>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" requestValidationMode="4.5" />
    <httpHandlers>
      <add path="*.less" verb="GET" type="dotless.Core.LessCssHttpHandler, dotless.Core" />
    </httpHandlers>
    <globalization uiCulture="auto" culture="auto" />
  </system.web>
